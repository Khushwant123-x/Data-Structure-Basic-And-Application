// Insertion Sort
#include <stdio.h>

int main() {
   int key,arr[]={
       1,4,6,8,2,3,9,0,45,67,89};
   int n= sizeof(arr)/sizeof(arr[0]);
   for(int i=1;i<n;i++){
       key=arr[i];
       int j=i-1;
       while(j>=0 && arr[j]>key){
           arr[j+1]=arr[j];
           j--;
       }
       arr[j+1]=key;
   }
    printf("\nSorted array:   ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }



    return 0;
}
